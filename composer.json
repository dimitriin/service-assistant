{
  "name": "dimitriin/php-service-assistant-lib",
  "description": "PHP integration library with Service Assistant",
  "keywords": ["php", "kubernetes", "health", "readiness", "prometheus", "metrics"],
  "homepage": "https://github.com/dimitriin/service-assistant",
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitrii Nasedkin",
      "email": "dimitrii.nasedkin@gmail.com"
    }
  ],
  "require": {
    "clue/socket-raw": "^1.4",
    "google/protobuf": "^3.13",
    "tuleap/prometheus-client": "^1.1",
    "psr/log": "^1.1"
  },
  "autoload":  {
    "psr-4": {
      "": "php-service-assistant-lib/gen",
      "Dimitriin\\": "php-service-assistant-lib/src"
    }
  },
  "archive": {
    "exclude": ["/.docker", "/.helm", "/.proto", "/.skaffold", "/cmd", "/pkg", "go.mod", "go.sum"]
  }
}
